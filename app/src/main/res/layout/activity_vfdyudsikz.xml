<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bg_raykinite"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".hbvgf.Juisouxhhucx">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Open Funny Fruits card and take scores"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cardView"
        android:layout_width="300dp"
        android:layout_height="450dp"
        app:cardCornerRadius="15dp"
        app:strokeColor="@color/black"
        app:strokeWidth="4dp"
        app:cardBackgroundColor="@color/bg_kyan_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.13">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/gkofo"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="24dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/bg_kya_item" />

            <ImageView
                android:id="@+id/cokvkocv"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintBottom_toBottomOf="@+id/gkofo"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/gkofo"
                app:srcCompat="@drawable/bg_kya_item" />

            <ImageView
                android:id="@+id/xioxkokxckox"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginTop="16dp"
                app:layout_constraintEnd_toEndOf="@+id/gkofo"
                app:layout_constraintStart_toStartOf="@+id/gkofo"
                app:layout_constraintTop_toBottomOf="@+id/gkofo"
                app:srcCompat="@drawable/bg_kya_item" />

            <ImageView
                android:id="@+id/dfokdfod"
                android:layout_width="120dp"
                android:layout_height="120dp"
                app:layout_constraintEnd_toEndOf="@+id/cokvkocv"
                app:layout_constraintStart_toStartOf="@+id/cokvkocv"
                app:layout_constraintTop_toTopOf="@+id/xioxkokxckox"
                app:srcCompat="@drawable/bg_kya_item" />

            <ImageView
                android:id="@+id/gokghohgok"
                android:layout_width="120dp"
                android:layout_height="120dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/xioxkokxckox"
                app:layout_constraintStart_toStartOf="@+id/xioxkokxckox"
                app:layout_constraintTop_toBottomOf="@+id/xioxkokxckox"
                app:srcCompat="@drawable/bg_kya_item" />

            <ImageView
                android:id="@+id/xijcxjixcijicjsygs"
                android:layout_width="120dp"
                android:layout_height="120dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.84"
                app:layout_constraintStart_toEndOf="@+id/gokghohgok"
                app:layout_constraintTop_toBottomOf="@+id/dfokdfod"
                app:srcCompat="@drawable/bg_kya_item" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <TextView
        android:id="@+id/xokxcko"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fruits score : 0"
        android:textColor="@color/white"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintVertical_bias="0.506" />

</androidx.constraintlayout.widget.ConstraintLayout>